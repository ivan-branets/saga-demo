{
  "defaultSeverity": "error",
  "extends": [
    "tslint:latest"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": false,
    "trailing-comma": false,
    "no-submodule-imports": false,
    "object-literal-sort-keys": false,
    "object-literal-key-quotes": false,
    "member-access": false,
    "arrow-parens": false,
    "max-line-length": [
      true,
      {
        "limit": 150
      }
    ]
  },
  "rulesDirectory": []
}